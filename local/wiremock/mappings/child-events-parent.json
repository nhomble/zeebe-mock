{
  "request": {
    "method": "POST",
    "url": "/CHILD_TRIGGERS_PARENT"
  },
  "response": {
    "jsonBody": {
      "command": "COMPLETE"
    }
  },
  "serveEventListeners": [
    {
      "name": "zeebemock/publish-message",
      "requestPhases": [
        "BEFORE_RESPONSE_SENT"
      ],
      "parameters": {
        "messageName": "PARENT_AWAITS_CHILD",
        "correlationKey": "{{ jsonPath request.body '$.variablesAsMap.id' }}",
        "variables": {
          "child": "trigger event"
        },
        "timeToLive": "PT10S"
      }
    }
  ],
  "metadata": {
    "zeebemock": {
      "enabled": true
    }
  }
}