{
  "request": {
    "method": "POST",
    "url": "/TRIGGER_EVENT"
  },
  "response": {
    "jsonBody": {
      "command": "COMPLETE",
      "variables": {
        "child": "trigger event"
      }
    }
  },
  "serveEventListeners": [
    {
      "name": "zeebemock/publish-message",
      "requestPhases": [
        "BEFORE_RESPONSE_SENT"
      ],
      "parameters": {
        "messageName": "TEST_EVENT",
        "correlationKey": "{{ jsonPath request.body '$.variablesAsMap.id' }}",
        "timeToLive": "PT10S"
      }
    }
  ],
  "metadata": {
    "zeebemock": {
      "enabled": true
    }
  }
}